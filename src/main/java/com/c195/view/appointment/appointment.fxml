<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.c195.controller.appointment.AppointmentController">
    <TableView fx:id="appointmentTable" layoutX="-4.0" layoutY="1.0" prefHeight="312.0" prefWidth="802.0">
        <columns>
            <TableColumn fx:id="titleColumn" prefWidth="150.0" text="title"/>
            <TableColumn fx:id="locationColumn" prefWidth="150.0" text="location"/>
            <TableColumn fx:id="customerNameColumn" prefWidth="150.0" text="customer"/>
            <TableColumn fx:id="typeColumn" prefWidth="150.0" text="type"/>
            <TableColumn fx:id="startColumn" prefWidth="100.0" text="start"/>
            <TableColumn fx:id="endColumn" prefWidth="100.0" text="end"/>
        </columns>
    </TableView>
    <Button onAction="#create" layoutX="30.0" layoutY="330.0" mnemonicParsing="false" prefHeight="48.0" prefWidth="122.0" text="Add"/>
    <Button onAction="#update" layoutX="180.0" layoutY="330.0" mnemonicParsing="false" prefHeight="48.0" prefWidth="122.0" text="Modify"/>
    <Button onAction="#delete" layoutX="330.0" layoutY="330.0" mnemonicParsing="false" prefHeight="48.0" prefWidth="122.0" text="Delete"/>
    <Button onAction="#cancel" layoutX="485.0" layoutY="330.0" mnemonicParsing="false" prefHeight="48.0" prefWidth="122.0" text="Cancel"/>
    <ComboBox fx:id="appointmentViewComboBox" layoutX="640" layoutY="330.0" prefHeight="48.0" prefWidth="122.0"/>
</AnchorPane>